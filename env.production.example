# WalletConnect Project ID (required)
# Get one at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Infura API Key (required for multi-chain support)
# Get one at https://infura.io/
NEXT_PUBLIC_INFURA_API_KEY=your_infura_api_key

# Alchemy API Key (optional but recommended for better RPC support)
# Get one at https://www.alchemy.com/
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key

# Chain Configuration
# These are the default contract addresses for each chain
# You can override them here if you've deployed your own contracts
NEXT_PUBLIC_ETHEREUM_BATCH_CONTRACT=0x...
NEXT_PUBLIC_POLYGON_BATCH_CONTRACT=0x...
NEXT_PUBLIC_ARBITRUM_BATCH_CONTRACT=0x...
NEXT_PUBLIC_OPTIMISM_BATCH_CONTRACT=0x...
NEXT_PUBLIC_BSC_BATCH_CONTRACT=0x...
NEXT_PUBLIC_AVALANCHE_BATCH_CONTRACT=0x...
NEXT_PUBLIC_GNOSIS_BATCH_CONTRACT=0x... 